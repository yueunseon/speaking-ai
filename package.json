{
	"name": "speaking-ai",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"sync": "svelte-kit sync",
		"clean": "rm -rf .svelte-kit node_modules/.vite",
		"clean:win": "if exist .svelte-kit rmdir /s /q .svelte-kit && if exist node_modules\\.vite rmdir /s /q node_modules\\.vite",
		"fix-types": "npm run clean:win && npm run sync",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^24",
		"@vitest/browser-playwright": "^4.0.15",
		"eslint": "^9.39.1",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"playwright": "^1.57.0",
		"svelte": "^5.45.6",
		"tailwindcss": "^4.1.17",
		"vite": "^7.2.6",
		"vitest": "^4.0.15",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@openai/agents": "^0.4.1",
		"@supabase/supabase-js": "^2.91.0",
		"openai": "^6.16.0",
		"vercel": "^50.4.9"
	}
}
